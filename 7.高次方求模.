#include<stdio.h>
int main()
{
    int x,y,m,t;//æ±‚t=x^y mod m;
    scanf("%d %d %d",&x,&y,&m);
    if(y==0) printf("1");
    else 
    {
        if(y==1) printf("%d",x%m);
        else
        {
            int t=1;
            for(int i=y;i>0;i--)
            t=t*x%m;
            
            printf("%d",t);
        }
    }
    return 0;
}
